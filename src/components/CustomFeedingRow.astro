---
type Props = {
  created: string;
  foodName: string;
  quantity: number;
};
const { created, foodName, quantity } = Astro.props;

const feedingTime = new Date(created).toLocaleTimeString(["fr-FR"], {
  hour: "2-digit",
  minute: "2-digit",
});

export const partial = true;
---

<li>
  <mark>
    <span>{feedingTime}</span> -
    <span>{quantity} g</span> -
    <span>{foodName}</span>
  </mark>
</li>
